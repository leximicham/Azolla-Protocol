{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "output_bundle",
  "description": "An immutable output artifact bundle from one diazotroph run.",
  "type": "object",
  "required": [
    "output_id",
    "ticket_id",
    "branch_name",
    "git_patch",
    "notes",
    "pr_description_draft",
    "created_at"
  ],
  "properties": {
    "output_id": {"type": "string"},
    "ticket_id": {"type": "string"},
    "branch_name": {"type": "string"},
    "git_patch": {"type": "string"},
    "notes": {"type": "string"},
    "pr_description_draft": {"type": "string"},
    "created_at": {"type": "string", "format": "date-time"}
  },
  "additionalProperties": false
}
